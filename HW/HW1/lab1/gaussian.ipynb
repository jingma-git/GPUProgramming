   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def gaussian(x, mean, std):\n",
    "    return 1.0 / np.sqrt(2.0 * np.pi * std**2) * np.exp(-(x-mean)**2 / (2*std**2))\n",
    "\n",
    "x = np.linspace(-10, 10, 101)\n",
    "y = gaussian(x, 0.0, 1.0)\n",
    "plt.plot(x, y)"
   ]
  }
